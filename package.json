{
  "name": "p-test-js",
  "version": "1.0.16",
  "engines": {
    "node": "12"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "description": "For parameterized javascript unit testing",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "rm:test": "find . -name '*.test.*' -type f -delete",
    "link": "tsc && cp package.json dist && cp README.md dist && cd dist && npm run rm:test && npm link",
    "dist": "tsc && cp package.json dist && cp README.md dist && cd dist && npm run rm:test && npm publish",
    "test": "mocha --require ts-node/register/transpile-only src/**/*.test.ts",
    "test:cov": "nyc mocha --require ts-node/register/transpile-only src/**/*.test.ts",
    "push": "git push && git tag v1.0.15 && git push origin v1.0.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasamachenkaow/parameterized-unit-tests-helper.git"
  },
  "author": "Kasama Chenkaow",
  "license": "ISC",
  "keywords": [
    "parameterised",
    "parameterized",
    "test",
    "tests",
    "testing",
    "unit test",
    "unit tests",
    "unit testing",
    "mocha"
  ],
  "bugs": {
    "url": "https://github.com/kasamachenkaow/parameterized-unit-tests-helper/issues"
  },
  "homepage": "https://github.com/kasamachenkaow/parameterized-unit-tests-helper#readme",
  "dependencies": {
    "mocha": "7.1.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "nyc": "^15.0.1",
    "p-test-js": "1.0.16",
    "sinon": "^9.0.2",
    "ts-node": "8.6.2",
    "ts-sinon": "^1.2.0",
    "typescript": "3.7.5"
  }
}
